    {
        <MudItem xs="6" sm="4">
            <img style="border-radius: 5px"
                 src="@(item["image"])" width="100%" height="100%"/>
        </MudItem>
    }
    </MudGrid>  
    <MudOverlay @bind-Visible="isVisible" Style="text-align: center" DarkBackground="true" ZIndex="9999" AutoClose="true">
        <img style="border-radius: 5px;width: 95vw;height: 80vh" src="@(overlayImage)" />
    </MudOverlay>

